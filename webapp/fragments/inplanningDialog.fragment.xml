<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog
    id="inplanningDialog"
    title="Inplanning Shipment"
    contentWidth="auto"
    contentHeight="auto"
  >
    <VBox>
      <Label text="Tknum:" />
      <Text id="tknumText" />
      <Label text="Time In:" />
      <TimePicker id="timeInPicker" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" />
      <Label text="Time Out:" />
      <TimePicker id="timeOutPicker" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" />
      <Label text="App Time:" />
      <TimePicker id="appTimePicker" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" />
      <Label text="Remarks:" />
      <TextArea id="remarksTextArea" />
    </VBox>
    <beginButton>
      <Button text="Close" press=".onCloseInplanningDialog" />
    </beginButton>
  </Dialog>
</core:FragmentDefinition>
