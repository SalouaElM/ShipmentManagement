<core:FragmentDefinition
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
>
    <m:Dialog
        title="Items"
        type="Standard"
    >
        <m:Table
    id="deliveryTabledd"
	 items="{path: 'ShipmentSet>DeliverySet>DeliveryItemSet/results'}"
>  
<m:headerToolbar>
        <m:OverflowToolbar>
            <m:Title text="Delivery Items" level="H2"/>
            <m:ToolbarSpacer/>
            
        </m:OverflowToolbar>
    </m:headerToolbar>
    <m:columns>
        <m:Column
            id="vbeln_Col"
            width="12em">
            <m:VBox>
                <m:Text text="DeliveryNr" />
            </m:VBox>
        </m:Column>
		<m:Column
            id="posnr_Col"
            width="12em">
            <m:VBox>
                <m:Text text="ItemNr" />
            </m:VBox>
        </m:Column>
        <m:Column
            id="matnr_Col"
            width="12em">
            <m:VBox>
                <m:Text text="MaterialNr" />
            </m:VBox>
        </m:Column>
		  <m:Column
            id="vrkme_Col"
            width="12em">
            <m:VBox>
                <m:Text text="Sales Unit" />
            </m:VBox>
        </m:Column>
        <m:Column
            id="lfimg_Col"
            width="12em">
            <m:VBox>
                <m:Text text="Delivered Quantity" />
            </m:VBox>
        </m:Column>
    </m:columns>
    <m:items>
        <m:ColumnListItem vAlign="Middle">
            <m:cells>
                <m:ObjectIdentifier title="{Vbeln}" />
                <m:ObjectIdentifier title="{Posnr}" />
				<m:Text text="{Matnr}" />
				<m:Text text="{Vrkme}" />
                <m:Text text="{Lfimg}" />
            </m:cells>
        </m:ColumnListItem>
    </m:items>
</m:Table>
<m:HBox justifyContent="End" class="buttonstyle">
    <m:Button text="Close" press=".onCloseDialog"/>
  </m:HBox>
    </m:Dialog>
</core:FragmentDefinition>