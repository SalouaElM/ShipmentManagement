<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem
				text="Planning Point"
				key="Tplst"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="Germany" key="DE00" />
					<ViewSettingsItem text="United Stated" key="US00" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsCustomItem text="Planned Date" key="PlannedDate">
			<customControl>
				<DatePicker id="planneddate"
        			value="{ path: 'PlannedDate', type: 'sap.ui.model.odata.type.Date' }"/>
			</customControl>
			</ViewSettingsCustomItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>